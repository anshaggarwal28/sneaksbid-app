{
  "version": 2,
  "builds": [
    {
      "src": "/snekasbiddjangoProject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "100mb" , "runtime":  "python3.9"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },

    {
      "src": "/(.*)",
      "dest": "/snekasbiddjangoProject/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "sneaksbid-app.settings"

  },
  "regions": ["all"]
}
